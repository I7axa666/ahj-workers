{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "no-plusplus": "off",
      "no-alert": "off",
      "no-tabs": "off",
      "no-unused-vars": "off",
      "no-console": "off",
      "no-restricted-globals": "off",
      "import/no-extraneous-dependencies": ["error", {"peerDependencies": true}]
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "parser": "@babel/eslint-parser"
}